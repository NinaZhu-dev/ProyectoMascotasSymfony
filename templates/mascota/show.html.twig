{% block title %}Mascota{% endblock %}

{% block body %}
    <h1>Mascota</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mascota.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ mascota.nombre }}</td>
            </tr>
            <tr>
                <th>Num_chip</th>
                <td>{{ mascota.numChip }}</td>
            </tr>
